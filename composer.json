{
    "name": "alfrecuellar/eloquent-model-generator",
    "description": "Auto-generates all Eloquent models from the database in a Laravel 5 project.",
    "keywords": ["laravel", "model", "eloquent", "artisan"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pepijn Olivier",
            "email": "olivier.pepijn@gmail.com"
        },
        {
            "name": "Alfredo Cuellar",
            "email": "alfrecuellar@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=4.1",
        "xethron/migrations-generator": "dev-l5"
    },
    "autoload": {
        "psr-4": {
            "AlfreCuellar\\EloquentModelGenerator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "mockery/mockery": ">=0.9.0",
        "illuminate/cache": ">=4.1.0",
        "illuminate/console": ">=4.1.0"
    }
}
